{
  "resources": {
    "head": [
      {
        "type": "style",
        "src": "./vendor/select-theme-default.css"
      },
      {
        "type": "style",
        "src": "./vendor/select-theme-dark.css"
      },
      {
        "type": "style",
        "src": "./vendor/select-theme-chosen.css"
      },
      {
        "type": "script",
        "src": "./vendor/tether.min.js"
      },
      {
        "type": "script",
        "src": "./vendor/select.min.js"
      },
      {
        "type": "script",
        "src": "./build/app.js"
      }
    ]
  },
  "preview": {
    "handlers": [
      {
        "options": ["*"],
        "execute": "INSTALL_SCOPE.setOptions(INSTALL_OPTIONS)"
      }
    ]
  },
  "options": {
    "properties": {
      "theme": {
        "order": 1,
        "title": "Theme",
        "type": "string",
        "default": "select-theme-default",
        "enum": [
          "select-theme-default",
          "select-theme-dark",
          "select-theme-chosen"
        ],
        "enumNames": {
          "select-theme-default": "Light",
          "select-theme-dark": "Dark",
          "select-theme-chosen": "Compact"
        }
      }
    }
  }
}
